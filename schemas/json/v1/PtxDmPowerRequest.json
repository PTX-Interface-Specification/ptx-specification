{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/PtxDmPowerRequest",
    "definitions": {
        "PtxDmPowerRequest": {
            "required": [
                "msg_header",
                "extension",
                "comm_request",
                "bulk_request"
            ],
            "properties": {
                "msg_header": {
                    "$ref": "#/definitions/PtxHeader",
                    "additionalProperties": true
                },
                "extension": {
                    "type": "string",
                    "description": "[RFC 3339, no decimals]",
                    "format": "date-time"
                },
                "comm_request": {
                    "type": "boolean"
                },
                "bulk_request": {
                    "type": "boolean"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Ptx Dm Power Request"
        },
        "PtxHeader": {
            "required": [
                "timestamp",
                "version"
            ],
            "properties": {
                "timestamp": {
                    "type": "string",
                    "description": "[RFC 3339, 3 decimals]",
                    "format": "date-time"
                },
                "version": {
                    "type": "integer",
                    "description": "initial version = 1"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Ptx Header"
        }
    }
}