{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/PtxDmVersion",
    "definitions": {
        "DmModuleVersion": {
            "required": [
                "module_class",
                "name",
                "version"
            ],
            "properties": {
                "module_class": {
                    "enum": [
                        "CLASS_UNKNOWN",
                        "CLASS_HW",
                        "CLASS_FW",
                        "CLASS_OS",
                        "CLASS_SW",
                        "CLASS_DATA",
                        "CLASS_CFG"
                    ],
                    "type": "string",
                    "title": "Dm Device Module Class Enum"
                },
                "name": {
                    "type": "string"
                },
                "version": {
                    "type": "string"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Dm Module Version"
        },
        "PtxDmVersion": {
            "required": [
                "msg_header",
                "description"
            ],
            "properties": {
                "msg_header": {
                    "$ref": "#/definitions/PtxHeader",
                    "additionalProperties": true
                },
                "description": {
                    "type": "string"
                },
                "module": {
                    "items": {
                        "$ref": "#/definitions/DmModuleVersion"
                    },
                    "type": "array"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Ptx Dm Version"
        },
        "PtxHeader": {
            "required": [
                "timestamp",
                "version"
            ],
            "properties": {
                "timestamp": {
                    "type": "string",
                    "description": "[RFC 3339, cf. ยง5.2]",
                    "format": "date-time"
                },
                "version": {
                    "type": "string",
                    "description": "current version = 2.0.0"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Ptx Header"
        }
    }
}